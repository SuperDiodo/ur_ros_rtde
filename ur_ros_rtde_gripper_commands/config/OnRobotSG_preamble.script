  def sg_width(target_width,gentle_grip,tool_index=0,blocking=True,depth_compensation=False,is_grip=False):
  if ON_DEBUG_LOG:
  textmsg("SG Grip start..")
  end 
  local retVal=0
  if not sg_Initialized_arr[tool_index]:
  sg_initialize(tool_index,sg_tool_id_arr[tool_index])
  end 
  if depth_compensation:
  local start_depth_mm=sg_Depth_arr[tool_index]
  end 
  retVal=on_tool_xmlrpc.sg_grip(tool_index,target_width,gentle_grip,is_grip)
  if(retVal!=0):
  popup(sg_grip_return_error,sg_grip_error_title,error=True,blocking=False)
  halt
  end 
  if blocking:
  if depth_compensation:
  sg_depth_compensate(tool_index,start_depth_mm)
  else:
  sg_wait_for_move(tool_index)
  end 
  end 
  sync()
  return retVal
  end 
  
