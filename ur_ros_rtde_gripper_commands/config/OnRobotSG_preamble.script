  end
  end
  local after_ready_continue_count=10
  while True:
  local busy=sg_Busy_arr[tool_index]
  if not busy:
  if after_ready_continue_count>0:
  after_ready_continue_count=after_ready_continue_count-1
  else:
  break
  end
  end
  end
  end
